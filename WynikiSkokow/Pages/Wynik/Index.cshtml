@page
@model WynikiSkokow.Pages.Wynik.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Skoczek[0].Imie)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Skoczek[0].Nazwisko)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Skoczek[0].Kraj)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Skoczek[0].Skok1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Skoczek[0].Skok2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Skoczek[0].Punkty)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Skoczek[0].Miejsce)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Skoczek)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Imie)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Nazwisko)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Kraj)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Skok1)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Skok2)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Punkty)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Miejsce)
                </td>
                <td>
                    <a asp-page="./Edit" asp-route-id="@item.ID">Edit</a> |
                    <a asp-page="./Details" asp-route-id="@item.ID">Details</a> |
                    <a asp-page="./Delete" asp-route-id="@item.ID">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
